<div class="panel panel-block">
  <div class="panel-heading"><h4 class="panel-title">{{ title | Default:'Links' }}</h4></div>
  <div class="list-group">
  {% for page in Page.Pages %}
    <a class="list-group-item" href="{{ page.Url }}"><i class="fa-fw {{ page.IconCssClass }}"></i>&nbsp; {{ page.Title }}</a>
  {% endfor %}
  </div>
</div>